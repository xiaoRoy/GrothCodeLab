<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/card_complete"
    android:layout_width="match_parent"
    android:layout_height="@dimen/d_card_height"
    android:layout_marginLeft="@dimen/d_card_margin_horizontal"
    android:layout_marginRight="@dimen/d_card_margin_horizontal"
    android:layout_marginTop="@dimen/d_card_margin_top"
    app:cardUseCompatPadding="true"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/s_card_completed"
            />
    </RelativeLayout>
</androidx.cardview.widget.CardView>