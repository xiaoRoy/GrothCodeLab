<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.learn.growthcodelab">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <uses-feature android:name="android.hardware.location.gps" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DataBindingActivity" />
        <activity android:name=".activity.WebViewActivity" />
        <activity
            android:name=".activity.FitSystemWindowActivity"
            android:theme="@style/NoActionBar" />
        <activity android:name=".activity.RecyclerActivity" />
        <activity android:name=".activity.MeasurementActivity" />
        <activity android:name=".activity.DrawableActivity" />
        <activity android:name=".activity.PlayGroundActivity" />
        <activity android:name=".activity.LayoutActivity" />
        <activity android:name=".activity.ViewPagerActivity" />
        <activity android:name=".activity.TransitionActivity" />
        <activity
            android:name=".fullscreen.FullScreenActivity"
            android:theme="@style/NoActionBar" />
        <activity android:name=".aboutfragment.FragmentActivity" />
        <activity android:name=".handler.HandlerActivityJ" />
        <activity android:name=".handler.HandlerActivity" />
        <activity android:name=".touchagain.TouchAgainActivity" />
        <activity
            android:name=".search.SearchActivity"
            android:theme="@style/SearchActivity" />
        <activity android:name=".window.WindowActivity" />
        <activity android:name=".viewshowcase.ViewShowcaseActivity" />
        <activity android:name=".architecture.guide.ui.UserProfileActivity" />
        <activity android:name=".architecture.jetpack.word.ui.WordsActivity" />
        <activity android:name=".fragment.FragmentPlayGroundActivity" />
        <activity
            android:name=".window.WindowInsetActivity"
            android:theme="@style/FullScreen" />
        <activity android:name=".search.SearchableActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity
            android:name=".window.drawer.DrawerActivity"
            android:theme="@style/FullScreen" />
        <activity android:name=".search.DictionaryActivity">
            <meta-data
                android:name="android.app.default_searchable"
                android:value=".search.SearchableActivity" />
        </activity>
        <activity
            android:name=".architecture.jetpack.courtcounter.CourtCounterActivity"
            android:theme="@style/CourtCounter" />
        <activity android:name=".architecture.mvvmlive.addedittask.AddEditTaskActivity" />
        <activity
            android:name=".viewshowcase.toolbar.ToolbarActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".jetpack.livedata.shared.ArticleActivity" />
        <activity android:name=".activityfield.DuckActivity" />
        <activity android:name=".rx.cheese.CheeseActivity" />
        <activity
            android:name=".navigation.NavigationActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Navigation" />
        <activity
            android:name=".material.MaterialActivity"
            android:theme="@style/MaterialTheme"/>
        <activity android:name=".mvx.mvc.view.BookListActivity" />
        <activity
            android:name=".viewshowcase.viewpager2.ViewPager2Activity"
            android:theme="@style/Theme.MaterialComponents.Light" />
        <activity android:name=".jetpack.lifecycle.LifeCycleActivity" />

        <receiver android:name=".navigation.DeepLinkAppWidgetProvider">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
            </intent-filter>
            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/app_widget_info_deep_link" />
        </receiver>
    </application>

</manifest>